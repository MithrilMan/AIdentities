{
   "DetailedErrors": true,
   "Serilog": {
      "Using": [ "Serilog.Sinks.Console" ],
      "MinimumLevel": {
         "Default": "Debug",
         "Override": {
            "Microsoft": "Warning",
            "System": "Warning"
         }
      }
   },
   "AIdentities": {
      "PackageFolder": "packages",
      "Chat": {
         "OpenAI": {
            "Enabled": true,
            "EndPoint": "https://api.openai.com/v1/chat/completions",
            "ApiKey": "Do-not-store-your-key-here-use-usersecrets-or-environment-variables",
            "DefaultModel": "gpt-3.5-turbo",
            "Timeout": "45000"
         }
      }
   }
}
